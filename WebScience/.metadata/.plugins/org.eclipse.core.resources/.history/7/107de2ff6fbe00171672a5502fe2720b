
import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.FileWriter;
import java.io.IOException;
import java.io.File;
import java.util.ArrayList;
import java.lang.StringBuilder;
import java.util.HashMap;

public class test {

    public static void main(String[] args) {
		
		ArrayList<Tweet> list = createList(args[0]);
		

    }

	private static ArrayList<Tweet> createList(String csvFile){
		
		BufferedReader br = null;
		String line = " ";
		String csvSplitBy = ",";
		ArrayList<Tweet> csvList = new ArrayList<Tweet>();

		try {
            br = new BufferedReader(new FileReader(csvFile));
            while ((line = br.readLine()) != null) {
				String[] row = line.split(csvSplitBy);
				csvList.add(new Tweet(Integer.parseInt(row[1]),row[2],Integer.parseInt(row[3]),Integer.parseInt(row[4]),Integer.parseInt(row[5]),row[6],row[7]));
            }
        } catch (FileNotFoundException e) {
            e.printStackTrace();
        } catch (IOException e) {
            e.printStackTrace();
        } finally {
            if (br != null) {
                try {
                    br.close();
                } catch (IOException e) {
                    e.printStackTrace();
                }
            }
        }
		return csvList;
	}
	
	private static HashMap<Integer,Cluster> createClusterMap(ArrayList<Tweet> tweet_list){
		
		HashMap<Integer,Cluster> cluster_map = new HashMap<Integer,Cluster>();
		int i = 0;
		
		while(i < tweet_list.size()){
			Tweet tweet = tweet_list.get(i);
			if(cluster_map.containsKey(tweet.getClusterID()) == false){
				cluster_map.put(tweet.getClusterID(),new Cluster(tweet.getClusterName()));
			}
			

		}
		 return cluster_map;
		
	}

}