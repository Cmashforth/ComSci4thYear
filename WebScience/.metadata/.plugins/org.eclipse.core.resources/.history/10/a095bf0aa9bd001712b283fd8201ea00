public class Cluster{
	
	private String cluster_name;
	private int size;
	private ArrayList<Tweet> tweet_list;
	
	
	public Cluster(int cn){
		cluster_id = ci;
		cluster_name = cn;
		size = 0;
		tweet_list = new ArrayList<Tweet>();
	}
	
	public void add_tweet(Tweet tweet){
		tweet_list.add(tweet);
		size = size + 1;
	}
	
	public int getSize(){
		return size;
	}
	
}